<?xml version="1.0" encoding="utf-8"?>
<odoo>
		<record model="ir.ui.view" id="syndic_quotities_type_tree">
			<field name="name">Type quotitee tree</field>
			<field name="model">syndic.building.quotities.type</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
				</tree>
			</field>
		</record>


		<!--form -->
		<record model="ir.ui.view" id="syndic_quotities_type_form">
			<field name="name">Type quotitee form</field>
			<field name="model">syndic.building.quotities.type</field>
			<field name="arch" type="xml">
				<form>
					<field name="name"/>
                </form>
			</field>
		</record>

		<record model="ir.actions.act_window" id="syndic_quotities_type_action">
			<field name="name">Type de quotitees</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">syndic.building.quotities.type</field>
			<field name="view_mode">tree,form</field>
		</record>

        <menuitem name="Type de quotitees" id="syndic_quotities_type_menu" parent="configuration_syndic" action="syndic_quotities_type_action"/>

</odoo>
