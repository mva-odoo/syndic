                    <li class="active" data-slide-to="0" data-target="#myCarousel0"/>
                  </ol>
                  <div class="carousel-inner">
                    <div class="item oe_img_bg active" style="background-image: url(http://odoo.sgimmo.be/website/image/ir.attachment/237_017aa7b/datas);">
                      <div class="container">
                        <div class="row content">
                          <div class="carousel-img col-md-6 hidden-sm hidden-xs"/>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="carousel-control left hidden" data-slide="prev" data-target="#myCarousel0" href="#myCarousel0" style="width: 10%">
                    <span tabindex="-1" data-cke-widget-wrapper="1" data-cke-filter="off" class="cke_widget_wrapper cke_widget_inline" data-cke-display-name="i" data-cke-widget-id="9">
                      <i class="fa fa-chevron-left cke_widget_element" data-cke-widget-data="{}" data-cke-widget-keep-attr="0" data-widget="icons"/>
                    </span>
                  </div>
                  <div class="carousel-control right hidden" data-slide="next" data-target="#myCarousel0" href="#myCarousel0" style="width: 10%">
