<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="syndic_document_building_form">
        <field name="name">Syndic document Building</field>
            <field name="model">syndic.building</field>
            <field name="inherit_id" ref="syndic_base.form_simpl_building"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='open_sign']" position="before">
                    <button name="get_document" type="object" class="oe_stat_button" icon="fa-archive" string="Documents"/>
                </xpath>
            </field>
    </record>
</odoo>
